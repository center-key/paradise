{
   "name": "paradise",
   "version": "0.3.2",
   "description": "Paradise PHP Photo Gallery",
   "summary": "A simple and aesthetic image gallery to showcase your art portfolio",
   "license": "GPL-3.0",
   "main": "gulpfile.js",
   "repository": "github:center-key/paradise",
   "homepage": "https://github.com/center-key/paradise",
   "bugs": "https://github.com/center-key/paradise/issues",
   "docs": "https://centerkey.com/paradise",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "artwork",
      "image-gallery",
      "images",
      "javascript",
      "php",
      "paradise",
      "photo-gallery",
      "photos",
      "portfolio",
      "gallery"
   ],
   "jshintConfig": {
      "strict": "implied",
      "esversion": 6,
      "undef": true,
      "unused": true,
      "node": true,
      "browser": true,
      "jquery": true,
      "globals": {
         "admin": true,
         "library": true,
         "dna": false,
         "gallery": true
      }
   },
   "scripts": {
      "pretest": "jshint gulpfile.js src/gallery/scripts/*.js src/gallery/console/scripts/*.js",
      "test": "gulp clean && gulp build && gulp zip"
   },
   "dependencies": {
      "crypto-js": "~3.1",
      "dna.js": "~1.4",
      "fetch-json": "~2.1",
      "jquery": "~3.3",
      "web-ignition": "~0.0"
   },
   "devDependencies": {
      "@babel/core": "~7.1",
      "@babel/preset-env": "~7.1",
      "cssnano": "~4.1",
      "del": "~3.0",
      "gulp": "~4.0",
      "gulp-babel": "~8.0",
      "gulp-concat": "~2.6",
      "gulp-file-include": "~2.0",
      "gulp-header": "~2.0",
      "gulp-less": "~4.0",
      "gulp-postcss": "~8.0",
      "gulp-replace": "~1.0",
      "gulp-size": "~3.0",
      "gulp-zip": "~4.2",
      "jshint": "~2.9",
      "merge-stream": "~1.0",
      "postcss-preset-env": "~6.3"
   }
}
