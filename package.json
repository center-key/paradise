{
   "name": "paradise",
   "version": "0.5.9",
   "description": "Paradise PHP Photo Gallery",
   "license": "GPL-3.0",
   "type": "module",
   "main": "gulpfile.js",
   "repository": "github:center-key/paradise",
   "homepage": "https://github.com/center-key/paradise",
   "bugs": "https://github.com/center-key/paradise/issues",
   "docs": "https://centerkey.com/paradise",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "artwork",
      "image-gallery",
      "images",
      "javascript",
      "php",
      "paradise",
      "photo-gallery",
      "photos",
      "portfolio",
      "gallery"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "node": true,
      "browser": true,
      "jquery": true,
      "globals": {
         "admin": true,
         "gallery": true,
         "globalThis": true,
         "libX": false,
         "dna": false
      }
   },
   "scripts": {
      "step:01": "rimraf build releases/paradise-v*.zip **/.DS_Store",
      "step:02": "jshint . --exclude-path .gitignore",
      "step:03": "lessc src/gallery/style/gallery.less       build/1-dev/gallery/paradise.css",
      "step:04": "lessc src/gallery/console/style/admin.less build/1-dev/gallery/console/paradise-console.css",
      "step:05": "make-dir build/2-min/gallery/console",
      "step:06": "csso build/1-dev/gallery/paradise.css                 --output build/2-min/gallery/paradise.min.css",
      "step:07": "csso build/1-dev/gallery/console/paradise-console.css --output build/2-min/gallery/console/paradise-console.min.css",
      "step:08": "uglifyjs src/gallery/js/*.js         --output build/2-min/gallery/paradise.min.js",
      "step:09": "uglifyjs src/gallery/console/js/*.js --output build/2-min/gallery/console/paradise-console.min.js",
      "step:10": "gulp build-php",
      "step:11": "add-dist-header build/2-min/gallery build/3-tst/gallery",
      "step:12": "add-dist-header build/2-min/gallery/console build/3-tst/gallery/console",
      "step:13": "copy-file src/gallery/image/.htaccess --folder build/3-tst/gallery/image",
      "step:14": "rev-web-assets build/3-tst/gallery build/4-rev/gallery",
      "step:15": "copy-file src/gallery/image/.htaccess --folder build/3-tst/gallery/image",
      "step:16": "copy-file LICENSE.txt --folder build/4-rev/gallery",
      "step:17": "cd build/3-tst && bestzip paradise.zip gallery/*",
      "step:18": "copy-file build/3-tst/paradise.zip releases/previous/paradise-v{{{pkg.version}}}.zip",
      "step:19": "copy-file build/3-tst/paradise.zip releases/paradise-v{{{pkg.version}}}.zip",
      "test": "npm-run-all step:*"
   },
   "cdnVersion": {
      "dnajs": "2.0",
      "dropzone": "5.9",
      "fetchJson": "2.6",
      "fontAwesome": "6.2",
      "jQuery": "3.6",
      "magnificPopup": "1.1",
      "webIgnition": "1.5"
   },
   "dependencies": {
      "dna.js": "~2.0",
      "fetch-json": "~2.6",
      "jquery": "~3.6",
      "web-ignition": "~1.5"
   },
   "devDependencies": {
      "@fortawesome/fontawesome-free": "~6.2",
      "add-dist-header": "~0.3",
      "bestzip": "~2.2",
      "copy-file-util": "~0.1",
      "copy-folder-cli": "~0.1",
      "cssnano": "~5.1",
      "csso-cli": "~4.0",
      "dropzone": "~5.9",
      "gulp": "~4.0",
      "gulp-file-include": "~2.3",
      "gulp-replace": "~1.1",
      "gulp-size": "~4.0",
      "gulp-sort": "~2.0",
      "jshint": "~2.13",
      "less": "~4.1",
      "magnific-popup": "~1.1",
      "make-dir-cli": "~3.0",
      "merge-stream": "~2.0",
      "npm-run-all2": "~6.0",
      "rev-web-assets": "~0.1",
      "rimraf": "~3.0",
      "uglify-js": "~3.17"
   }
}
