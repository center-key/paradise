{
   "name": "paradise",
   "version": "0.2.1",
   "description": "Paradise PHP Photo Gallery",
   "summary": "A simple and aesthetic image gallery to showcase your art portfolio",
   "license": "GPL-3.0",
   "main": "gulpfile.js",
   "repository": "github:center-key/paradise",
   "homepage": "https://github.com/center-key/paradise",
   "bugs": "https://github.com/center-key/paradise/issues",
   "docs": "https://centerkey.com/paradise",
   "keywords": [
      "php",
      "paradise",
      "photo",
      "gallery"
   ],
   "author": {
      "name": "Center Key",
      "url": "https://centerkey.com"
   },
   "jshintConfig": {
      "strict": "implied",
      "esversion": 6,
      "undef": true,
      "unused": true,
      "node": true,
      "browser": true,
      "jquery": true,
      "globals": {
         "admin": true,
         "library": true,
         "dna": false,
         "gallery": true
      }
   },
   "scripts": {
      "pretest": "jshint gulpfile.js src/gallery/scripts/*.js src/gallery/console/scripts/*.js",
      "test": "gulp clean && gulp build && gulp zip"
   },
   "devDependencies": {
      "babel-core": "~6.26",
      "babel-preset-env": "~1.7",
      "cssnano": "~4.0",
      "del": "~3.0",
      "gulp": "~3.9",
      "gulp-babel": "~7.0",
      "gulp-concat": "~2.6",
      "gulp-file-include": "~2.0",
      "gulp-header": "~2.0",
      "gulp-less": "~4.0",
      "gulp-postcss": "~8.0",
      "gulp-rename": "~1.4",
      "gulp-replace": "~1.0",
      "gulp-size": "~3.0",
      "gulp-zip": "~4.2",
      "jshint": "~2.9",
      "merge-stream": "~1.0",
      "postcss-preset-env": "~5.3"
   }
}
