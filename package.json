{
   "name": "paradise",
   "version": "0.4.4",
   "description": "Paradise PHP Photo Gallery",
   "summary": "A simple and aesthetic image gallery to showcase your art portfolio",
   "license": "GPL-3.0",
   "main": "gulpfile.js",
   "repository": "github:center-key/paradise",
   "homepage": "https://github.com/center-key/paradise",
   "bugs": "https://github.com/center-key/paradise/issues",
   "docs": "https://centerkey.com/paradise",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "artwork",
      "image-gallery",
      "images",
      "javascript",
      "php",
      "paradise",
      "photo-gallery",
      "photos",
      "portfolio",
      "gallery"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "node": true,
      "browser": true,
      "jquery": true,
      "globals": {
         "admin": true,
         "gallery": true,
         "library": false,
         "dna": false
      }
   },
   "scripts": {
      "pretest": "jshint *.js src",
      "test": "gulp clean-target && gulp build-app && gulp make-zip"
   },
   "cdnVersion": {
      "dnajs": "1.6",
      "dropzone": "5.7",
      "fetchJson": "2.2",
      "fontAwesome": "5.14",
      "jQuery": "3.5",
      "magnificPopup": "1.1",
      "webIgnition": "1.2"
   },
   "dependencies": {
      "dna.js": "~1.6",
      "fetch-json": "~2.2",
      "jquery": "~3.5",
      "web-ignition": "~1.2"
   },
   "devDependencies": {
      "@babel/core": "~7.10",
      "@babel/preset-env": "~7.10",
      "@fortawesome/fontawesome-free": "~5.14",
      "babel-minify": "~0.5",
      "babel-preset-minify": "~0.5",
      "cssnano": "~4.1",
      "del": "~5.1",
      "dropzone": "~5.7",
      "gulp": "~4.0",
      "gulp-append-prepend": "~1.0",
      "gulp-babel": "~8.0",
      "gulp-concat": "~2.6",
      "gulp-file-include": "~2.2",
      "gulp-header": "~2.0",
      "gulp-less": "~4.0",
      "gulp-postcss": "~8.0",
      "gulp-replace": "~1.0",
      "gulp-size": "~3.0",
      "gulp-sort": "~2.0",
      "gulp-zip": "~5.0",
      "jshint": "~2.11",
      "magnific-popup": "~1.1",
      "merge-stream": "~2.0",
      "postcss-preset-env": "~6.7"
   }
}
